
{
  "runtime": {
    "version": "Lua 5.4",
    "path": [
      "?.lua",
      "?/init.lua",
      "Assets/Scripts/?.lua",
      "Assets/Scripts/?/init.lua"
    ]
  },
  "workspace": {
    "library": [
      "Assets/Scripts",
      "EngineAPI"
    ],
    "checkThirdParty": false
  },
  "diagnostics": {
    "globals": ["Engine", "Scene", "Input", "Time", "Log"]
  },
  "hint": {
    "enable": true
  }
}
